<?xml version="1.0"?>
<!DOCTYPE database SYSTEM "litesql.dtd"> 
<database name="Hive" namespace="test">
    <object name="File">
<!--        <field name="id" type="integer"/>-->
        <field name="filename" type="string"/>
    </object>
    <object name="Packet">
        <field name="streamid" type="integer"/>
        <field name="pts" type="integer"/>
        <field name="dts" type="integer"/>
        <field name="streamindex" type="integer"/>
        <field name="keyframe" type="integer"/>
        <field name="framegroup" type="integer"/>
        <field name="flags" type="integer"/>
        <field name="duration" type="integer"/>
        <field name="pos" type="integer"/>
        <field name="datasize" type="integer"/>
        <field name="data" type="string"/>
    </object>
</database>
