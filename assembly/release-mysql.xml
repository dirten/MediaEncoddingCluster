<assembly>
  <id>${my.family}-${build_type}</id>
  <formats>
    <format>zip</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>
  <fileSets>
    <fileSet>
      <directory>source/build/mysql/lib/${mysql.lib.dir}</directory>
      <outputDirectory>lib</outputDirectory>
      <includes>
        <include>mysqlclient.lib</include>
        <include>mysqlclient.pdb</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>source/build/mysql/lib/mysql</directory>
      <outputDirectory>lib</outputDirectory>
      <includes>
        <include>libmysqlclient.a</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>source/build/mysql/libexec/</directory>
      <outputDirectory>bin</outputDirectory>
      <includes>
        <include>mysqld</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>source/build/mysql/bin</directory>
      <outputDirectory>bin</outputDirectory>
      <includes>
        <include>mysqld.exe</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>source/build/mysql/include/mysql</directory>
      <outputDirectory>include/mysql</outputDirectory>
      <includes>
        <include>*.*</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>source/build/mysql/include</directory>
      <outputDirectory>include/mysql</outputDirectory>
      <includes>
        <include>*.*</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>source/build/mysql/</directory>
      <outputDirectory></outputDirectory>
      <includes>
        <include>share/english/*.*</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>source/build/mysql/share/mysql</directory>
      <outputDirectory>share/</outputDirectory>
      <includes>
        <include>english/*.*</include>
        <include>mysql_system_tables.sql</include>
        <include>mysql_system_tables_data.sql</include>
      </includes>
    </fileSet>
  </fileSets>
</assembly>
