<assembly>
  <id>${my.family}-${build_type}</id>
  <formats>
    <format>zip</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>
  <fileSets>
    <fileSet>
      <directory>source/build/safmq/include</directory>
      <outputDirectory>include/safmq</outputDirectory>
      <includes>
        <include>**/*</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>source/tmp</directory>
      <outputDirectory>include/safmq</outputDirectory>
      <includes>
        <include>ForwardThread.h</include>
        <include>Log.h</include>
        <include>Mutex.h</include>
        <include>QManager.h</include>
        <include>ServerThread.h</include>
        <include>StreamCache.h</include>
        <include>SystemConnection.h</include>
        <include>SystemDelivery.h</include>
        <include>param_config.h</include>
        <include>safmq-Signal.h</include>
        <include>thdlib.h</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>source/tmp</directory>
      <outputDirectory>include/safmq/lib</outputDirectory>
      <includes>
        <include>MQConnection.h</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>source/build/safmq/lib</directory>
      <outputDirectory>lib</outputDirectory>
      <includes>
        <include>**/*</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>source/safmq.0.8.3/build</directory>
      <outputDirectory>lib</outputDirectory>
      <includes>
        <include>**/*.pdb</include>
      </includes>
    </fileSet>
  </fileSets>
</assembly>
